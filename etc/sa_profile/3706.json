{
    "process": "download_server",
    "systemability": [
        {
            "name": 3706,
            "libpath": "libdownload_server.dylib.so",
            "run-on-create": false,
            "distributed": false,
            "dump_level": 1,
            "min_hdi_proxy_version": [],
            "recycle-strategy": "low-memory",
            "start-on-demand": {
                "allow-update": true,
                "commonevent": [
                    {
                        "name": "usual.event.USER_REMOVED"
                    }
                ]
            },
            "stop-on-demand": {
                "param": [
                    {
                        "name": "resourceschedule.memmgr.low.memory.prepare",
                        "value": "true"
                    }
                ]
            }
        }
    ]
}